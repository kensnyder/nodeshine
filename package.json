{
  "name": "nodeshine",
  "version": "2.0.0",
  "description": "A compatibility layer to run Bunshine on Node",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "npm run build && vitest run",
    "coverage": "npm run build && vitest run --coverage",
    "build": "node scripts/build.mjs",
    "generate-dts": "cp ./node_modules/bunshine/index.d.ts ./index.d.ts",
    "lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/nodeshine.git"
  },
  "keywords": [
    "Bunshine for Node",
    "Bunshine",
    "HTTP Server"
  ],
  "author": "Ken Snyder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kensnyder/nodeshine/issues"
  },
  "homepage": "https://github.com/kensnyder/nodeshine#readme",
  "devDependencies": {
    "@types/node": "22.6.1",
    "@vitest/coverage-v8": "2.1.1",
    "bun-types": "1.1.29",
    "esbuild": "0.24.0",
    "eventsource": "2.0.2",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "type-fest": "4.26.1",
    "typescript": "5.6.2",
    "vitest": "2.1.1"
  },
  "dependencies": {
    "bunshine": "2.0.0",
    "mime": "4.0.4"
  }
}
