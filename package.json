{
  "name": "nodeshine",
  "version": "0.12.11",
  "description": "A compatibility layer to run Bunshine on Node",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "npm run build && vitest run",
    "coverage": "npm run build && vitest run --coverage",
    "build": "node scripts/build.mjs",
    "generate-dts": "rm index.d.ts; bunx dts-bundle-generator -o index.d.ts index.ts",
    "lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/nodeshine.git"
  },
  "keywords": [
    "Bunshine for Node",
    "Bunshine",
    "HTTP Server"
  ],
  "author": "Ken Snyder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kensnyder/nodeshine/issues"
  },
  "homepage": "https://github.com/kensnyder/nodeshine#readme",
  "devDependencies": {
    "@types/bun": "1.0.3",
    "@types/node": "20.11.5",
    "@vitest/coverage-v8": "1.2.1",
    "esbuild": "0.19.11",
    "eventsource": "2.0.2",
    "prettier": "3.2.4",
    "prettier-plugin-organize-imports": "3.2.4",
    "type-fest": "4.9.0",
    "typescript": "5.3.3",
    "vitest": "1.2.1"
  },
  "dependencies": {
    "bunshine": "0.13.0",
    "mime": "4.0.1"
  }
}
